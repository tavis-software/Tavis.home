<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>json-home</AssemblyName>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <VersionPrefix>1.0.0</VersionPrefix>
    <Authors>Darrel Miller</Authors>
    <Description>.NET Support for application/json-home</Description>
    <PackageTags>asp.net core;.net core;hypermedia;rest;application/json-home</PackageTags>
    <PackageProjectUrl>https://github.com/tavis-software/Tavis.home</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="Tavis.Link" Version="2.5.0" />
  </ItemGroup>
  

</Project>